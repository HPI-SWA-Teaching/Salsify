.courseInfoBox {
	border: 1px solid #e5e5e5;
	padding: 0.5cm 1cm;
	background: #F3F3F3;
	position: relative;
	margin-bottom: 5mm;
}

.groupselect {
	min-width: 15em;
	margin: 3mm;
	padding: 2mm;
}

.tdLeft {
	min-width: 12em;
}

.submitComponent .date {
	display:block;
	margin-top: -1.6em;
	margin-right: 26%;
	text-align:center;
	font-weight:normal;
}

div.submitComponent, div.submission {
	padding: 0px;
}

.submitComponent .title {
	padding: 0px;
	width: 6cm;
}

.submitComponent em {
	font-size: 10pt;
	font-style: normal;
	padding-left: 1em;
}

.submitComponent td input[type="submit"], .submitComponent a.button {
	margin: 0;
}

.submitComponent a.button {
	padding: 1.2mm 3mm;
	display:block;
}

.submitComponent h4 {
	margin-bottom: 2mm;
	margin-top: 1cm;
}

.submitComponent .uploader {
	width: 400px;
}


.submitComponent .uploader a, .tdText {
	display:block;
	padding: 5px 5px 0 5px;
	word-wrap: break-word;
	width: 390px;
	padding: 2mm 3mm;
}

.submissionInfoBox {
	border: 1px solid #e5e5e5;
	padding: 0.5cm 1cm;
	background: #F3F3F3;
	position: relative;
	margin-bottom: 2mm;
}

table {  
	border-spacing: 0px;
 }

 .tdText {
	display:block;
	padding: 10px 5px 0 5px;
	word-wrap: break-word;
	width: 390px;
}



.zipDownload {
	display: block;
	margin-left: 5mm;
	font-size: 10pt;

}

.inlineLabel {
	display: inline!important;
	margin: 0!important;
}

#groupOverview td {
	padding: 2mm;
}

#groupOverview thead td {
	font-weight:bold;
}

.submissionHeading {  
	margin-top: 0.5cm;
 }

